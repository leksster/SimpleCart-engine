<p id="notice"><%= notice %></p>

<h1>Listing Kks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Qty</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kks.each do |kk| %>
      <tr>
        <td><%= kk.name %></td>
        <td><%= kk.price %></td>
        <td><%= kk.qty %></td>
        <td><%= link_to 'Show', kk %></td>
        <td><%= link_to 'Edit', edit_kk_path(kk) %></td>
        <td><%= link_to 'Destroy', kk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kk', new_kk_path %>
